					"listen": "test",
					"script": {
						"exec": [
							"var reqBody= JSON.parse(pm.request.body.raw);",
							"var result=pm.response.json();",
							"",
							"if(pm.response.code==200) {",
							"    msg=\"Đăng nhập thành công\";",
							"    pm.environment.set(\"token1\",result.token);",
							"    pm.test(\"Đăng nhập thành công\",function() {",
							"        pm.expect(result.message).to.eql(msg);",
							"        pm.expect(reqBody.account).to.eql(result.user.account);",
							"        pm.expect(pm.response.text()).to.include(\"id\");",
							"    })",
							"};",
							"",
							"pm.test(\"Tài khoản người dùng không tồn tại\",function(){",
							"pm.response.to.have.status(404);",
							"pm.expect(pm.response.text()).to.include(\"message\");",
							"});",
							"",
							"pm.test(\"Mật khẩu không hợp lệ\",function(){",
							"    pm.response.to.have.status(400);",
							"});",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
